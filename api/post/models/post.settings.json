{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string"
    },
    "coverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "subtext": {
      "type": "text"
    },
    "mainBody": {
      "type": "richtext"
    },
    "createdBy": {
      "type": "component",
      "repeatable": false,
      "component": "created.created-by"
    },
    "slug": {
      "type": "string"
    },
    "category": {
      "via": "posts",
      "model": "category"
    },
    "keywords": {
      "type": "text"
    }
  }
}
