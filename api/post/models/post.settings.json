{
  "kind": "collectionType",
  "collectionName": "posts",
  "info": {
    "name": "Post"
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string"
    },
    "CoverImage": {
      "model": "file",
      "via": "related",
      "allowedTypes": [
        "images",
        "files",
        "videos"
      ],
      "plugin": "upload",
      "required": false,
      "pluginOptions": {}
    },
    "Subtext": {
      "type": "text"
    },
    "MainBody": {
      "type": "richtext"
    },
    "CreatedBy": {
      "type": "component",
      "repeatable": false,
      "component": "created.created-by"
    }
  }
}
